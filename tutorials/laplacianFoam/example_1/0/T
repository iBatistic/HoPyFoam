FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 1;

boundaryField
{
    bottom
    {
        type            fixedValue;
        value	        uniform 0;
    }

    top
    {
        type            fixedValue;
        value	        uniform 0;
    }

    left
    {
        type            fixedValue;
        value	        uniform 0;
    }
    
    right
    {
        type            fixedValue;
        value	        uniform 1;
    }

    frontAndBackPlanes
    {
        type            empty;
    }
}


